properties([[$class: 'JiraProjectProperty'], gitLabConnection(''), [$class: 'GitlabLogoProperty', repositoryName: ''], giteeConnection(''), [$class: 'RebuildSettings', autoRebuild: false, rebuildDisabled: false], parameters([string(defaultValue: '3', description: '', name: 'StringA', trim: false), string(defaultValue: '4', description: '', name: 'StringB', trim: false)]), pipelineTriggers([githubPush()])])
	 
node(){

	environment {
                w = "5" 	
                    }

	  stage("1-Add two numbers") {
		
	echo "select the first number ${params.StringA}"
	echo "select the second number ${params.StringB}"
	
	def x = ${params.StringA};
	def y = ${params.StringB};
	
	z = x + y;
	
	echo "${z}"
            
	    }    
	  
	  stage("2-If Loop") {
	  
	  if (z > env.w) {
                echo 'The Build is a Stable'
            } else {
                echo 'The Build is UnStable'
            }                        
	}
    }
